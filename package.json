{
  "name": "@elizaos/plugin-tokenmetrics",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@elizaos/source": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos/eliza.git",
    "directory": "packages/plugin-tokenmetrics"
  },
  "keywords": [
    "elizaos",
    "crypto",
    "cryptocurrency",
    "tokenmetrics",
    "trading",
    "blockchain",
    "ai",
    "plugin",
    "market-data",
    "trading-signals"
  ],
  "author": "ElizaOS Contributors",
  "license": "MIT",
  "dependencies": {
    "@elizaos/core": "^0.25.9",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "tsup": "8.3.5",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "vitest": "^3.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --tsconfig ./tsconfig.json",
    "dev": "tsup src/index.ts --format esm --dts --watch --tsconfig ./tsconfig.json",
    "clean": "rm -rf dist/",
    "rebuild": "npm run clean && npm run build",
    "typecheck": "npx tsc --noEmit",
    "test:build": "npm run build && node -e \"import('./dist/index.js').then(plugin => { console.log('‚úÖ Plugin loaded successfully'); console.log('üìä Available actions:', Object.keys(plugin.default?.actions || {})); }).catch(err => { console.error('‚ùå Plugin load failed:', err.message); process.exit(1); })\"",
    "verify": "npm run typecheck && npm run test:build"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "TOKENMETRICS_API_KEY": {
        "type": "string",
        "minLength": 1,
        "description": "The plugin requires the following environment variable: TOKENMETRICS_API_KEY=your_tokenmetrics_api_key",
        "optional": false
      }
    }
  }
}
