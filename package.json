{
  "name": "@token-metrics-ai/plugin-tokenmetrics",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "@elizaos/source": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/token-metrics/plugin-tokenmetrics.git"
  },
  "keywords": [
    "elizaos",
    "crypto",
    "cryptocurrency",
    "tokenmetrics",
    "trading",
    "blockchain",
    "ai",
    "plugin",
    "market-data",
    "trading-signals"
  ],
  "author": "Token Metrics <support@tokenmetrics.com> (https://www.tokenmetrics.com/)",
  "license": "MIT",
  "description": "ElizaOS plugin for TokenMetrics API - provides comprehensive cryptocurrency data, AI insights, and trading signals for ElizaOS agents",
  "homepage": "https://github.com/token-metrics/plugin-tokenmetrics#readme",
  "bugs": {
    "url": "https://github.com/token-metrics/plugin-tokenmetrics/issues"
  },
  "dependencies": {
    "@elizaos/core": "latest",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "prettier": "^3.0.0",
    "bun": "^1.2.15",
    "@types/bun": "latest",
    "typescript": "^5.8.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "clean": "rm -rf dist .turbo node_modules .turbo-tsconfig.json tsconfig.tsbuildinfo",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "TOKENMETRICS_API_KEY": {
        "type": "string",
        "description": "API key for accessing TokenMetrics services",
        "required": true,
        "sensitive": true
      }
    }
  }
}
